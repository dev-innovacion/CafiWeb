<div class="widgets_area">
    <div class="span12 form_row ">
    </div>

    <div class="row-fluid">
        <div class="span12">
            <div class="light_blue set_color">
                <div class="well-header widgetclient titleclient">
                    <h5>Lista de Circuitos</h5>
                    <div class="btn-group">
                        <button class="btn btn-mini dark_green fileinput-button" id="addUserButton" data-original-title="Agregar Circuito" rel="tooltip" data-placement="top">
                            <i class="icon-plus icon-white"></i>
                        </button>
                        <button type="button" class="btn btn-mini red delete" id="deleteUserButton" data-original-title="Eliminar Circuitos" rel="tooltip" data-placement="top">
                            <i class="icon-trash icon-white"></i>
                        </button>
                        <button class="btn btn-mini dark_green" id="toTileViewButton" data-original-title="Mosaicos" rel="tooltip" data-placement="top">
                            <i class=" icon-th-large icon-white"></i>
                            <span></span>
                        </button>
                        <button class="btn btn-mini orange" id="toSliderViewButton" data-original-title="Slider" rel="tooltip" data-placement="top">
                            <i class=" icon-resize-horizontal icon-white"></i>
                            <span></span>
                        </button>
                        <button class="btn btn-mini light_blue" id="toTableViewButton" data-original-title="Tabla" rel="tooltip" data-placement="top">
                            <i class=" icon-th-list icon-white"></i>
                            <span></span>
                        </button>

                    </div>
                </div>
                <div class="well-content no-search" id="circuitsTable">
                </div>
            </div>
        </div>
    </div>
</div>

