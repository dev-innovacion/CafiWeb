            //Validate the required fields
            validateFields: function () {
                var valid = true;
                if (this.type == "movement" && this.temp == true) {
                    if (jQuery(".tempMovement input[name=verify_name]").val().trim() == "" ) {
                        _alert("error", "La información de quien verifica el daño es requerida");
                        valid = false;
                    } else {
                        valid = true;
                    }
                } else if (this.type == "movement") {
                    var objects = OrderTable.getObjects();
                    for (var ob in objects) {
                        if (objects[ob].quantity_new == "") {
                            valid = false;
                            _alert("error", "El valor tiene que ser numérico.");
                            OrderTable.table.find("tr").eq(objects[ob].orderNumber - 1).click();
                            break;
                        }
                        if (parseInt(objects[ob].quantity_new) < 1) {
                            valid = false;
                            _alert("error", "El valor no puede ser menor a uno.");
                            OrderTable.table.find("tr").eq(objects[ob].orderNumber - 1).click();
                            break;
                        }
                        if (parseInt(objects[ob].quantity) < parseInt(objects[ob].quantity_new)) {
                            valid = false;
                            _alert("error", "La cantidad es mayor a la cantidad original.");
                            OrderTable.table.find("tr").eq(objects[ob].orderNumber - 1).click();
                            break;
                        }
                        if (parseInt(objects[ob].quantity_new).toString() != objects[ob].quantity_new) {
                            valid = false;
                            _alert("error", "La cantidad debe ser un número entero.");
                            OrderTable.table.find("tr").eq(objects[ob].orderNumber - 1).click();
                            break;
                        }
                    }
                }
                else if (this.type == "create") {
                    var objects = OrderTable.getObjects();
                    for (var ob in objects) {
                        if (objects[ob].label == "" || objects[ob].serie == "" || objects[ob].department == "") {
                            valid = false;
                            _alert("error", "Información adicional es requerida");
                            OrderTable.table.find("tr").eq(objects[ob].orderNumber - 1).click();
                            break;
                        }
                    }
                }
                return valid;
            },

