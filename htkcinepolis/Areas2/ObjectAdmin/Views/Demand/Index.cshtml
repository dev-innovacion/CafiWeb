    function bindcheckall() {
        jQuery("#checkall").unbind("change.all");
        jQuery("#checkall").bind("change.all", function () {
           
            var table = jQuery("#results_rvtable").dataTable()
            var trs = table.fnGetNodes()
            if (jQuery("#checkall:checked").length == 0) {
                jQuery(trs).find("td input:checked").prop("checked", false);
            } else {
                jQuery(trs).find("td input:not(:checked)").prop("checked", true);
            }

            var cad = jQuery("#results_rvtable").find("td input:checked");
            jQuery(cad[cad.length - 1]).trigger("click");
            jQuery(cad[cad.length - 1]).prop("checked", true);
        })
    }
