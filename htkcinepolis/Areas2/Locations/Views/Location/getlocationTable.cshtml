@{Layout = null;}

<div class="row-fluid">
    <div class="span12">
        <div class="light_blue set_color">
            <div class="well-header widgetclient titleclient">
                <h5>Lista de Ubicaciones</h5>
            </div>
            <div class="well-content no-search">
                <table class="table table-striped table-bordered table-hover datatable" id="locationTable">
                    <thead>
                        <tr>
                            <th></th>
                            <th>Nombre</th>
                            <th>Nombre para mostrar</th>
                            <th>Perfil</th>
                            <th>Creador</th>
                            <th>Fecha de Creaci&oacute;n</th>
                            <th>&Uacute;ltima modificaci&oacute;n</th>
                            <th>Funciones</th>
                        </tr>
                    </thead>
                    <tbody>
                        @ViewData["locations"]
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>