        });


    </script>

    <script>
        var images = "";
        jQuery(document).ready(function ($) {
            $('.uniform').uniform();


        });

        jQuery.backstretch([
            @{int count = 0;}
            @foreach(string url in Model){

                if (count == 0)
                {
                 @(new HtmlString("'"+url+"'"));
                 count++;
                }
                else { 
               @(new HtmlString(",'"+url+"'"));
                }
            
              }
              /*  "/Content/Images/slider/slide_01.jpg",
                "/Content/Images/slider/slide_03.jpg",
                "/Content/Images/slider/slide_02.jpg",*/

        ], {
            duration: 4000, fade: 1000
        });
    </script>

