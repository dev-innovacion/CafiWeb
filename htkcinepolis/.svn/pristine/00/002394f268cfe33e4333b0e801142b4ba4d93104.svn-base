@using System;
@using System.Dynamic;
@{

    Layout = null;
}
@{
    List<dynamic> news = new List<dynamic>();
    List<dynamic> epcs = new List<dynamic>();
    List<dynamic> ids = new List<dynamic>();

    try { 
        news = ViewData["news"] as List<dynamic>;
    }
    catch { }
    try
    {
        epcs = ViewData["epcs"] as List<dynamic>;
    }
    catch { }
   try{
        ids = ViewData["ids"] as List<dynamic>;
   }
   catch { }
  
     
}
<div>
<div class="navbar-inner ">
    <ul class="nav nav-tabs" id="myTab">
      
       <li class="active" id="tab1"><a href="#newstab" data-toggle="tab">Nuevos Objetos</a></li>
        <li id="tab3"><a href="#dupidtab" data-toggle="tab">Objetos Modificados Via EPC</a></li>

       <li id="tab2"><a href="#duptab" data-toggle="tab">Objetos Modificados Via ID</a></li>
        
    </ul>
</div>
<div class="tab-content" >
    <div class="tab-pane active tabfirst perfilMargen" id="newstab">
        <div class="overOn contentHolder well-content no_padding row-fluid" style="min-height:290px; max-height: 290px; overflow: hidden">

            <table id="newstable" class="table table-striped table-hover dataTable datatable">

                <thead>
                    <tr class="widgetclient" style="color: white;">
                       
                        <th class="table-fixed-medium">ID Registro</th>
                        <th class="table-fixed-medium">Unidad Explotación</th>
                        <th class="table-fixed-medium">Nombre de Conjunto</th>
                        <th class="table-fixed-medium">Departamento</th>
                        <th class="table-fixed-medium">Ubicacion</th>
                        <th class="table-fixed-medium">ID Articulo</th>
                        <th class="table-fixed-medium">Descripcion Articulo</th>
                        <th class="table-fixed-medium">Marca</th>
                        <th class="table-fixed-medium">Modelo</th>
                        <th class="table-fixed-medium">#Serie</th>
                        <th class="table-fixed-medium">Epc</th>
                        <th class="table-fixed-medium">Estatus</th>
                        <th class="table-fixed-medium">Fecha Etiq.</th>
                        <th class="table-fixed-medium">Usuario Etiq.</th>
                        <th class="table-fixed-medium">Cantidad</th>
                        <th class="table-fixed-medium">ID Paquete</th>
                        <th class="table-fixed-medium">ID Ubicacion</th>
                        <th class="table-fixed-medium">ID Usuario</th>

                    </tr>
                </thead>
                <tbody>
                    @{int hnot = 0;}
                    @foreach (dynamic row in news)
                    {
                         
                       <tr class="mailtr">
                           
                            <td class="table-fixed-medium">@row.id_registro</td>
                            <td class="table-fixed-medium">@row.af_unidad_explotacion</td>
                            <td class="table-fixed-medium">@row.af_nombre_conjunto</td>
                            <td class="table-fixed-medium">@row.af_departamento</td>
                            <td class="table-fixed-medium">@row.af_ubicacion</td>
                            <td class="table-fixed-medium">@row.af_id_articulo</td>
                            <td class="table-fixed-medium">@row.af_desc_articulo</td>
                            <td class="table-fixed-medium">@row.af_marca</td>
                            <td class="table-fixed-medium">@row.af_modelo</td>
                            <td class="table-fixed-medium">@row.af_num_serie</td>
                            <td class="table-fixed-medium">@row.af_epc_completo</td>
                            <td class="table-fixed-medium">@row.af_status_etiquetado</td>
                            <td class="table-fixed-medium">@row.af_fecha_etiquetado</td>
                            <td class="table-fixed-medium">@row.af_usuario_etiquetado</td>
                            <td class="table-fixed-medium">@row.af_cantidad</td>
                            <td class="table-fixed-medium">@row.af_id_paquete</td>
                            <td class="table-fixed-medium">@row.ub_id_ubicacion</td>
                            <td class="table-fixed-medium">@row.id_usuario</td>
                        </tr>
                        hnot++;

                    }

                </tbody>

            </table>
            @if (news.Count() == 0)
            {
                <br /><br /><br />
                <center><h4>Tabla Sin Registros ..</h4></center>
            }
            

        </div>
        
      
    </div>
