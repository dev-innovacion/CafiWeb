@{Layout = null;}
@using Newtonsoft.Json;
@using Newtonsoft.Json.Linq;
@model JArray

<div class="divTree">
    <table class="table table-striped table-bordered table-hover datatable">
        <thead>
            <tr>
                <th>
                    @*<input data-ref="checkbox " type="checkbox" class="uniform" name="checkTitle">*@
                </th>
                <th>Nombre de Usuario</th>
                <th>Fecha de Creación</th>
            </tr>
        </thead>
        <tbody>
            @foreach (JObject user in Model)
            { 
                <tr data-profile="@user["profileId"]" data-id="@user["_id"]">
                    <td>
                        <input data-ref="checkbox " type="checkbox" class="checker">
                    </td>
                    <td>@user["name"] @user["lastname"]</td>
                    <td>@user["CreatedDate"]</td>
                </tr>
            }
        </tbody>
    </table>
</div>

