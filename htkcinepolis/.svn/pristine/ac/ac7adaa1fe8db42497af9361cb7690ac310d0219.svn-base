    function bindzoom(position) {
        position = typeof position !== 'undefined' ? position : 1;
        jQuery('.zoom').elevateZoom({
            //zoomType: "inner",
            zoomWindowPosition: position,
            cursor: "crosshair",
            scrollZoom: true,
            zoomWindowFadeIn: 500,
            zoomWindowFadeOut: 750
        });
    }
