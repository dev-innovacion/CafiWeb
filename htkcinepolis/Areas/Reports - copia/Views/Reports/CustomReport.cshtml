                if (listmailsf[i].value == movementsel) {

                    objresult.id = listmailsf[i].id;
                    objresult.value = movementsel;
                    validate = 1;

                    for (var j = 0; j < list_users.length; j++) {
                        if (list_users[j].id == objresult.id) {
                            reply = 1;
                            st = 0;
                            break;
                        } else {
                            reply = 0;
                            st = 1;

                        }
